{
  "name": "stix-cli",
  "version": "1.1.1",
  "description": "The CLI for stix framework.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpoonX/stix-cli.git"
  },
  "author": "RWOverdijk <r.w.overdijk@gmail.com>",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "api",
    "framework",
    "node",
    "cli",
    "stix"
  ],
  "bugs": {
    "url": "https://github.com/SpoonX/stix-cli/issues"
  },
  "homepage": "https://github.com/SpoonX/stix-cli",
  "scripts": {
