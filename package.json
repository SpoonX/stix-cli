    "postpublish": "git push upstream master && git push upstream --tags"
  },
  "bin": {
    "stix": "lib/index.js"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cli-spinner": "^0.2.8",
    "command-line-usage": "^5.0.5",
